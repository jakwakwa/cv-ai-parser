{
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "accessibility.verbosity.panelChat": false,
  "cursor.chat.terminalShowHoverHint": true,
  "cursor.cmdk.autoSelect": false,
  "cursor.composer.backspaceRemoveContext": false,
  "cursor.composer.cmdPFilePicker2": true,
  "cursor.composer.collapsePaneInputBoxPills": true,
  "cursor.composer.shouldShowMarkdownHoverParticipantActions2": false,
  "cursor.composer.showSuggestedFiles": false,
  "cursor.composer.subAgentsMode": "off",
  "cursor.composer.supportsReviewMode": false,
  "cursor.composer.textSizeScale": 1,
  "cursor.cpp.disabledLanguages": [
    "plaintext"
  ],
  "cursor.general.gitGraphIndexing": "enabled",
  "cursor.general.globalCursorIgnoreList": [
    "**/credentials.json",
    "**/credentials.*.json",
    "**/secret.json",
    "**/secrets.json",
    "**/*.key",
    "**/*.pem",
    "**/*.pfx",
    "**/*.p12",
    "**/*.crt",
    "**/*.cer",
    "**/id_rsa",
    "**/id_dsa",
    "**/.ssh/id_*"
  ],
  "deno.enablePaths": [
    "supabase/functions"
  ],
  "deno.lint": true,
  "deno.unstable": [
    "bare-node-builtins",
    "byonm",
    "sloppy-imports",
    "unsafe-proto",
    "webgpu",
    "broadcast-channel",
    "worker-options",
    "cron",
    "kv",
    "ffi",
    "fs",
    "http",
    "net"
  ],
  "editor.codeActionWidget.includeNearbyQuickFixes": false,
  "editor.codeLens": false,
  "editor.colorDecorators": false,
  "editor.colorDecoratorsActivatedOn": "click",
  "editor.comments.ignoreEmptyLines": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "Dank Mono, Monaco, 'Courier New', monospace",
  "editor.fontSize": 11,
  "editor.formatOnSave": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "eslint.workingDirectories": [
    {
      "mode": "auto"
    }
  ],
  "workbench.panel.showLabels": false,
  "workbench.view.showQuietly": {
    "workbench.panel.output": true
  },
  "yaml.maxItemsComputed": 50
}